{
    "env": {
        "es2021": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "class-methods-use-this": 0,
        "react/jsx-filename-extension": [
            "error",
            {
              "extensions": [".js", ".jsx"]
            }
        ]
    }
}